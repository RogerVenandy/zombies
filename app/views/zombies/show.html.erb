<div class="container">
  <p id="notice"><%= notice %></p>

  <p class="text-success">
    <strong>Killed:</strong>
    <%= @zombie.killed %>
  </p>

  <p>
    <strong>Name:</strong>
    <%= @zombie.name %>
  </p>

  <p>
    <strong>Health:</strong>
    <%= @zombie.health %>
  </p>

  <p>
    <strong>Photo:</strong>
    <%= @zombie.photo %>
  </p>

  <%= link_to 'Edit', edit_zombie_path(@zombie), class: "btn btn-primary  " %> |
  <%= link_to 'Back', zombies_path, class: "btn btn-warning" %>

</div>
